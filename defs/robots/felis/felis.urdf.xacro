<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="felis">
<xacro:include filename="$(find riserobotdb)/defs/robots/felis/felis.common.urdf.xacro" />
<!-- Base Link -->
<xacro:urdf_link _name="base_link" _xyz="${felis_xyz}" _rpy="${felis_rpy}"
	_mesh="${felis_base_mesh}" _mass="${felis_baselink_mass}" _cg="${felis_baselink_cg}"
	 _ixx="${felis_baselink_ixx}" _iyy="${felis_baselink_iyy}" _izz="${felis_baselink_izz}"
	_ixy="${felis_baselink_ixy}" _ixz="${felis_baselink_ixz}" _iyz="${felis_baselink_iyz}"/>
<!-- Spools -->
<xacro:spool suffix="left" portion="front" geom_rpy="${felis_spool_joint1_rpy}" joint_xyz="${felis_spool_joint1_xyz}" />
<xacro:spool suffix="right" portion="front" geom_rpy="${felis_spool_joint2_rpy}" joint_xyz="${felis_spool_joint2_xyz}" />
<xacro:spool suffix="left" portion="rear" geom_rpy="${felis_spool_joint3_rpy}" joint_xyz="${felis_spool_joint3_xyz}" />
<xacro:spool suffix="right" portion="rear" geom_rpy="${felis_spool_joint4_rpy}" joint_xyz="${felis_spool_joint4_xyz}" />
<!-- Legs -->
<xacro:quadleg suffix="left" portion="front" geom_rpy="${felis_linkA_joint1_rpy}" joint_xyz="${felis_linkA_joint1_xyz}" />
</robot>
